{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "clean": {
      "cache": false
    },
    "compile": {
      "dependsOn": ["^compile"]
    },
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
    },
    "test": {
      "outputs": ["coverage/**"]
    }
  }
}
