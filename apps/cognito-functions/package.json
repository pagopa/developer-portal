{
  "name": "cognito-functions",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf dist/",
    "compile": "npm run clean && tsc",
    "build": "esbuild dist/custom-message-handler.js* --bundle --platform=node --outfile=.out/index.js",
    "postbuild": "cd .out && zip -r index.zip index.js*",
    "lint": "eslint src",
    "test": "npm run compile && jest -i"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.125",
    "@types/rewire": "^2.5.28",
    "babel-plugin-rewire": "^1.2.0",
    "esbuild": "^0.19.5",
    "eslint": "^8.40.0",
    "eslint-config-custom": "*",
    "jest": "^29.5.0",
    "rewire": "^6.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
