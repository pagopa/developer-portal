{
  "name": "developer-portal",
  "version": "0.1.0",
  "private": true,
  "config": {
    "codegen": {
      "targetDir": "./src/adapters/gitbook/generated/client"
    },
    "openapi": {
      "gitbook": {
        "prod": "https://api.gitbook.com/openapi.yaml",
        "staging": "https://api.gitbook-staging.com/openapi.yaml"
      }
    }
  },
  "scripts": {
    "compile": "tsc",
    "test": "jest -i",
    "test:coverage": "yarn run test -- --coverage",
    "dev": "next dev",
    "build": "yarn generate:gitbook:client && next build",
    "start": "next start",
    "lint": "next lint",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "prepare": "husky install",
    "generate:gitbook:client": "shx rm -rf $npm_package_config_codegen_targetDir && shx mkdir -p $npm_package_config_codegen_targetDir && sta -p $npm_package_config_openapi_gitbook_prod -o $npm_package_config_codegen_targetDir --add-readonly --type-prefix GitBook --axios"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.119",
    "@mui/material": "^5.11.8",
    "@next/font": "13.1.6",
    "@pagopa/mui-italia": "^0.8.12",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "axios": "^1.3.3",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "fp-ts": "^2.13.1",
    "next": "13.1.6",
    "prettier": "^2.8.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "shx": "^0.3.4",
    "swagger-typescript-api": "^12.0.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  }
}
