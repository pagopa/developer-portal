{
  "name": "gitbook-docs",
  "version": "1.0.0",
  "private": true,
  "exports": {
    "./": "./dist/"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "shx rm -rf dist/",
    "compile": "npm run clean && tsc",
    "lint": "eslint src",
    "test": "jest -i",
    "generate-guides-metadata": "ts-node src/scripts/generateGuidesMetadata.ts",
    "generate-solutions-metadata": "ts-node src/scripts/generateSolutionsMetadata.ts",
    "generate-release-notes-metadata": "ts-node src/scripts/generateReleaseNotesMetadata.ts",
    "fetch-soap-api-from-strapi": "ts-node src/scripts/generateSoapApiRepositoriesList.ts",
    "generate-url-parsing-metadata": "ts-node src/scripts/generateUrlParsingMetadata.ts",
    "parse-docs": "ts-node src/scripts/parseDocUrlsAndIncludes.ts"
  },
  "dependencies": {
    "@markdoc/markdoc": "0.3.0",
    "dotenv": "^17.2.1",
    "emojibase-data": "^15.3.2",
    "fp-ts": "^2.13.1",
    "htmlparser2": "^9.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^13.0.1"
  },
  "devDependencies": {
    "@types/htmlparser2": "^3.10.3",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/markdown-it": "^12.2.3",
    "eslint": "^8.40.0",
    "eslint-config-custom": "*",
    "jest": "^29.5.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
